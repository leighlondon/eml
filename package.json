{
  "name": "eml",
  "displayName": "Email",
  "description": "Email syntax highlighting.",
  "version": "0.3.2",
  "publisher": "leighlondon",
  "icon": "etc/eml-icon.png",
  "author": {
    "name": "leighlondon"
  },
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/leighlondon/eml",
  "repository": {
    "type": "git",
    "url": "https://github.com/leighlondon/eml.git"
  },
  "galleryBanner": {
    "color": "#00B0FF",
    "theme": "light"
  },
  "contributes": {
    "languages": [
      {
        "id": "eml",
        "aliases": [
          "Email",
          "email",
          "eml"
        ],
        "extensions": [
          ".email",
          ".eml",
          ".msg",
          ".mbox"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eml",
        "scopeName": "text.eml",
        "path": "./syntax/eml.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.html": "html"
        }
      }
    ]
  }
}
