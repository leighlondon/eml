{
  "name": "eml",
  "displayName": "Email",
  "description": "Email syntax highlighting.",
  "version": "0.2.2",
  "publisher": "leighlondon",
  "icon": "etc/eml-icon.png",
  "author": {
    "name": "leighlondon"
  },
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": ["Languages"],
  "preview": true,
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/leighlondon/eml",
  "repository": {
    "type": "git",
    "url": "https://github.com/leighlondon/eml.git"
  },
  "galleryBanner": {
    "color": "#00B0FF",
    "theme": "light"
  },
  "activationEvents": ["onLanguage:eml"],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "eml",
        "aliases": ["Email", "email", "eml"],
        "extensions": [".email", ".eml", ".msg", ".mbox"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eml",
        "scopeName": "text.eml",
        "path": "./syntaxes/eml.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
