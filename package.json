{
  "name": "eml",
  "displayName": "Email Syntax",
  "description": "Email syntax highlighting.",
  "version": "0.1.2",
  "publisher": "leighlondon",
  "author": {
    "name": "leighlondon"
  },
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": ["Languages"],
  "preview": true,
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/leighlondon/eml",
  "repository": {
    "type": "git",
    "url": "https://github.com/leighlondon/eml.git"
  },
  "galleryBanner": {
    "color": "#64B5F6",
    "theme": "light"
  },
  "contributes": {
    "languages": [
      {
        "id": "eml",
        "aliases": ["Email", "email", "eml"],
        "extensions": [".email", ".eml", ".msg", ".mbox"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eml",
        "scopeName": "text.eml",
        "path": "./syntaxes/eml.tmLanguage.json"
      }
    ]
  }
}
